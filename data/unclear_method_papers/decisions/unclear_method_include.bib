	date = {2006},
	keywords = {abundance, Antarctica, Biodiversity, Carbon, Chlorophyll, Coastal zones, Dry Valleys, East Antarctica, Fungi, fungus, habitat use, Moisture, nutrient cycling, Nutrition, {pH}, {pH} effects, population distribution, Soil biology, soil moisture, Soils, Taylor Valley, Victoria Land, Victoria Lands, Yeast},
