@article{abbas_isolation_1987,
	title = {Isolation and Toxicity of Fusarium Species from Various Areas of Norway},
	volume = {37},
	url = {https://www.scopus.com/inward/record.uri?eid = 2-s2.0-0023544515&doi = 10.1080%2f00015128709436574&partnerID = 40&md5 = 69e821ebe5fe4a26d9d7fe0cc65025c5},
	doi = {10.1080/00015128709436574},
	abstract = {One hundred and twenty-five isolates of Fusarium were identified in ninety-five samples of soil collected from the arctic north, central and southern regions of Norway. Species were F. acuminatum (25 isolates, found in 26.3\% of the 95 soil samples), F. avenaceum (28 isolates,29.5\%),F.culmorum({\textbackslash} isolate, 1.1 \%),F. oxysporum(31 isolates,32.6\%),{andF}. sambucinum (40 isolates, 42.1\%). The isolates were grown on a solid rice substrate, incorporated as 50\% of a rodent diet and fed to rats to determine their toxicity. Sixty-four per cent of the isolates caused death and 44\% of the isolates caused congestion and hemorrhage of various tissues including stomach, intestine, thymus, and hematuria in the bladder. One isolate caused uterine enlargement. All isolates caused decreased weight gain relative to the control diets. The toxicity of Fusarium isolates recovered from the soil samples was remarkable, particularly in F. oxysporum (25 out of 31 (80\%) were toxic) and F. sambucinum (39/40 = 97\%) reported for the first time. Key words: Fusarium, mycotox-ins. © 1987, Taylor \& Francis Group, {LLC}. All rights reserved.},
	pages = {427--435},
	number = {4},
	journaltitle = {Acta Agriculturae Scandinavica},
