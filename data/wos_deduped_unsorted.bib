@article{ WOS:001272130700001,
  ADDRESS = {RADARWEG 29, 1043 NX AMSTERDAM, NETHERLANDS},
  AUTHOR = {Harder, Christoffer Bugge and Miyauchi, Shingo and Viragh, Mate and Kuo, Alan and Thoen, Ella and Andreopoulos, Bill and Lu, Dabao and Skrede, Inger and Drula, Elodie and Henrissat, Bernard and Morin, Emmanuelle and Kohler, Annegret and Barry, Kerrie and Labutti, Kurt and Salamov, Asaf and Lipzen, Anna and Merenyi, Zsolt and Hegedus, Botond and Baldrian, Petr and Stursova, Martina and Weitz, Hedda and Taylor, Andy and Koriabine, Maxim and Savage, Emily and Grigoriev, V, Igor and Nagy, Laszlo G. and Martin, Francis and Kauserud, Havard},
