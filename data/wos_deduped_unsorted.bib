  AUTHOR = {Mahaney, William C. and Hart, Kris M. and O'Reilly, Shane S. and Allen, Christopher C. R. and Dohm, James M. and Hancock, Ronald G. V. and Kelleher, Brian P. and Milner, Michael W.},
  JOURNAL = {PLANETARY AND SPACE SCIENCE},
  MONTH = {JAN},
