@article{ WOS:001107759400001,
  ADDRESS = {RADARWEG 29, 1043 NX AMSTERDAM, NETHERLANDS},
  AUTHOR = {Larsen, Stefano and Coleine, Claudia and Albanese, Davide and Stegen, James C. and Selbmann, Laura and Donati, Claudio},
  JOURNAL = {SCIENCE OF THE TOTAL ENVIRONMENT},
  MONTH = {JAN 10},
