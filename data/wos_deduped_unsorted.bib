  ISSN = {1462-2912},
  EISSN = {1462-2920},
  KEYWORDS.PLUS = {LAKE VOSTOK; ICE; BACTERIA; MICROORGANISMS; TEMPERATURE; GROWTH; MODEL; SOIL; PERMAFROST; RECOVERY},
