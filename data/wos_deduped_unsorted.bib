@article{ WOS:001242154800003,
  ADDRESS = {HEIDELBERGER PLATZ 3, BERLIN, 14197, GERMANY},
  AUTHOR = {Saez-Sandino, Tadeo and Maestre, Fernando T. and Berdugo, Miguel and Gallardo, Antonio and Plaza, Cesar and Garcia-Palacios, Pablo and Guirado, Emilio and Zhou, Guiyao and Mueller, Carsten W. and Tedersoo, Leho and Crowther, T. W. and Delgado-Baquerizo, Manuel},
  JOURNAL = {NATURE CLIMATE CHANGE},
  MONTH = {JUL},
  NUMBER = {7},
  PUBLISHER = {NATURE PORTFOLIO},
  TITLE = {Increasing numbers of global change stressors reduce soil carbon worldwide},
  TYPE = {Article},
  VOLUME = {14},
  YEAR = {2024},
  ABSTRACT = {Soils support a vast amount of carbon (C) that is vulnerable to climatic and anthropogenic global change stressors (for example, drought and human-induced nitrogen deposition). However, the simultaneous effects of an increasing number of global change stressors on soil C storage and persistence across ecosystems are virtually unknown. Here, using 1,880 surface soil samples from 68 countries across all continents, we show that increases in the number of global change stressors simultaneously exceeding medium-high levels of stress (that is, relative to their maximum levels observed in nature) are negatively and significantly correlated with soil C stocks and mineral association across global biomes. Soil C is particularly vulnerable in low-productivity ecosystems (for example, deserts), which are subjected to a greater number of global change stressors exceeding medium-high levels of stress simultaneously. Taken together, our work indicates that the number of global change stressors is a crucial factor for soil C storage and persistence worldwide. Soil carbon storage is vulnerable to various climatic and anthropogenic global change stressors (for example drought, warming, land-use intensification). Here the authors show that multiple stress factors act simultaneously to reduce soil carbon storage and persistence across global biomes.},
  LANGUAGE = {English},
  AFFILIATION = {Saez-Sandino, T (Corresponding Author), Univ Pablo Olavide, Dept Sistemas Fis Quim \& Nat, Seville, Spain. Saez-Sandino, T (Corresponding Author), Western Sydney Univ, Hawkesbury Inst Environm, Penrith, NSW, Australia. Delgado-Baquerizo, M (Corresponding Author), CSIC, Lab Biodiversidad \& Funcionamiento Ecosistem, Inst Recursos Nat \& Agrobiol Sevilla IRNAS, Seville, Spain. Saez-Sandino, Tadeo; Gallardo, Antonio, Univ Pablo Olavide, Dept Sistemas Fis Quim \& Nat, Seville, Spain. Saez-Sandino, Tadeo, Western Sydney Univ, Hawkesbury Inst Environm, Penrith, NSW, Australia. Maestre, Fernando T., King Abdullah Univ Sci \& Technol, Biol \& Environm Sci \& Engn Div, Environm Sci \& Engn, Thuwal, Saudi Arabia. Berdugo, Miguel, Univ Complutense Madrid, Dept Biodiversidad Ecol \& Evoluc, Madrid, Spain. Plaza, Cesar; Garcia-Palacios, Pablo, CSIC, Inst Ciencias Agr ICA, Madrid, Spain. Garcia-Palacios, Pablo, Univ Zurich, Dept Plant \& Microbial Biol, Zurich, Switzerland. Guirado, Emilio, Univ Alicante, Inst Multidisciplinar Estudio Medio Ramon Margalef, San Vicente Del Raspeig, Spain. Guirado, Emilio, Univ Alicante, Dept Ecol, San Vicente Del Raspeig, Spain. Zhou, Guiyao; Delgado-Baquerizo, Manuel, CSIC, Lab Biodiversidad \& Funcionamiento Ecosistem, Inst Recursos Nat \& Agrobiol Sevilla IRNAS, Seville, Spain. Zhou, Guiyao, German Ctr Integrat Biodivers Res iDiv, Leipzig, Germany. Mueller, Carsten W., Tech Univ Berlin, Inst Ecol, Berlin, Germany. Tedersoo, Leho, Univ Tartu, Mycol \& Microbiol Ctr, Tartu, Estonia. Crowther, T. W., Swiss Fed Inst Technol, Inst Integrat Biol, Dept Environm Syst Sci, Zurich, Switzerland.},
  DOI = {10.1038/s41558-024-02019-w},
