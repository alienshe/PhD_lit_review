  AUTHOR = {Hou, L. W. and Giraldo, A. and Groenewald, J. Z. and Raemae, T. and Summerbell, R. C. and Huang, G. Z. and Cai, L. and Crous, P. W.},
  JOURNAL = {STUDIES IN MYCOLOGY},
  MONTH = {JUN},
  NUMBER = {105},
  PAGES = {23-203},
