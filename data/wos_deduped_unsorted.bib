  AUTHOR = {Edwards, Arwyn and Douglas, Brian and Anesio, Alexandre M. and Rassner, Sara M. and Irvine-Fynn, Tristram D. L. and Sattler, Birgit and Griffith, Gareth W.},
  JOURNAL = {FUNGAL ECOLOGY},
