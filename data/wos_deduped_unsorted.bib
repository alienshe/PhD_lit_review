  AUTHOR = {Balascio, Nicholas L. and Anderson, R. Scott and D'Andrea, William J. and Wickler, Stephen and D'Andrea, Robert M. and Bakke, Jostein},
  JOURNAL = {HOLOCENE},
  MONTH = {DEC},
