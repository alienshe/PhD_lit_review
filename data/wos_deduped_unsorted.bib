  DOI = {10.3389/fmicb.2013.00260},
  EISSN = {1664-302X},
  KEYWORDS = {microbial biomass; extracellular enzyme; litter bag; decomposition; decomposer community; nutrients; turnover activity},
  KEYWORDS.PLUS = {ORGANIC-MATTER DECOMPOSITION; TEMPERATURE-DEPENDENCE; NITROGEN DEPOSITION; THEORETICAL-MODEL; SOIL RESPIRATION; ARCTIC TUNDRA; CARBON; FOREST; FUNGAL; DECAY},
