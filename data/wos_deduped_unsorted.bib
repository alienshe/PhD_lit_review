  RESEARCH.AREAS = {Plant Sciences},
  WEB.OF.SCIENCE.CATEGORIES = {Plant Sciences},
  AUTHOR.EMAIL = {bjorn.lindahl@slu.se},
  AFFILIATIONS = {Swedish University of Agricultural Sciences; Swedish University of Agricultural Sciences; Royal Netherlands Academy of Arts \& Sciences; Netherlands Institute of Ecology (NIOO-KNAW); Universite de Lorraine; INRAE},
