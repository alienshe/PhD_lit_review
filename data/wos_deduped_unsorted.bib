  AUTHOR = {Masumoto, Shota and Kitagawa, Ryo and Nishizawa, Keita and Osono, Takashi and Hasegawa, Motohiro and Iimura, Yasuo and Matsuoka, Shunsuke and Kaneko, Ryo and Uchida, Masaki and Mori, Akira S.},
