  KEYWORDS.PLUS = {HUMICOLA-LANUGINOSA LIPASE; ALPHA/BETA-HYDROLASE FOLD; SUBSTRATE-BINDING; RHIZOMUCOR-MIEHEI; CANDIDA-RUGOSA; ORGANIC MEDIA; DATA-BANK; COMPLEX; SPECIFICITY; PROTEIN},
  RESEARCH.AREAS = {Biochemistry \& Molecular Biology; Biophysics},
