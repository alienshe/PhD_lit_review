@article{ WOS:000649684900005,
  ADDRESS = {RADARWEG 29, 1043 NX AMSTERDAM, NETHERLANDS},
  AUTHOR = {Matsuoka, Shunsuke and Doi, Hideyuki and Masumoto, Shota and Kitagawa, Ryo and Nishizawa, Keita and Tanaka, Kaho and Hasegawa, Motohiro and Hobara, Satoru and Osono, Takashi and Mori, Akira S. and Uchida, Masaki},
  JOURNAL = {POLAR SCIENCE},
  MONTH = {MAR},
  PUBLISHER = {ELSEVIER},
  TITLE = {Taxonomic, functional, and phylogenetic diversity of fungi in a forest-tundra ecotone in Qu?ebec},
  TYPE = {Article},
  VOLUME = {27},
  YEAR = {2021},
  ABSTRACT = {We quantified the taxonomic, functional, and phylogenetic diversity of fungi associated with dead moss tissues and conifer needles in a forest-tundra ecotone in a sub-Arctic region, Quebec, Canada. We detected 615 operational taxonomic units, in total, of fungi in 84 families with 97\% sequence similarity by metabarcoding the internal transcribed spacer 1 (ITS1) region of nuclear ribosomal DNA. Saprotrophs were the dominant functional guild. The results indicated the effects of host plant species and local environmental heterogeneity on fungal diversity in the cold region. Taxonomic and phylogenetic diversity indices of fungal communities were significantly greater in moss than in conifers. The greater phylogenetic diversity in moss was mainly attributable to the presence of families in Mucoromycota, compared to those in conifers, in which relatively closely related families of Dikarya were present. The taxonomic diversity was also related to local variations in vegetation and soil properties, suggesting that the local colonization and establishment of vegetation and the concomitant development of soil contributed to the taxonomic diversity of fungi. In contrast, functional diversity was not significantly affected by the host, vegetation, or soil properties, indicating that the fungal community members were relatively redundant in terms of the evaluated functional traits.},
  LANGUAGE = {English},
  AFFILIATION = {Matsuoka, S (Corresponding Author), Univ Hyogo, Grad Sch Simulat Studies, Kobe, Hyogo 6500047, Japan. Matsuoka, Shunsuke; Doi, Hideyuki, Univ Hyogo, Grad Sch Simulat Studies, Kobe, Hyogo 6500047, Japan. Masumoto, Shota; Kitagawa, Ryo; Nishizawa, Keita; Mori, Akira S., Yokohama Natl Univ, Grad Sch Environm \& Informat Sci, Yokohama, Kanagawa 2408501, Japan. Tanaka, Kaho; Hasegawa, Motohiro; Osono, Takashi, Doshisha Univ, Fac Sci \& Engn, Dept Environm Syst Sci, Kyoto 6100394, Japan. Hobara, Satoru, Rakuno Gakuen Univ, Dept Environm Symbiot Sci, Ebetsu, Hokkaido 0698501, Japan. Uchida, Masaki, Natl Inst Polar Res, Tachikawa, Tokyo 1908518, Japan. Uchida, Masaki, Grad Univ Adv Studies, Tachikawa, Tokyo 1908518, Japan.},
