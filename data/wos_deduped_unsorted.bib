  KEYWORDS = {root nodules; arbuscular mycorrhizal fungi; hot spring settings; Araucariales; Jurassic},
  KEYWORDS.PLUS = {LA MATILDE FORMATION; SANTA-CRUZ PROVINCE; MOLECULAR PHYLOGENY; ANTARCTIC PENINSULA; DESEADO MASSIF; ARUM-TYPE; CLASSIFICATION; WOOD; PLANTS; BASIN},
