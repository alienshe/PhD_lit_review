  FUNDING.ACKNOWLEDGEMENT = {Kyoto University Foundation; Sumitomo Foundation, Program for Next Generation World-Leading Researcher {[}GS008]; Japan Society for Promotion of Science {[}26252020, 26550004, 17H06297, P09316]; National Natural Science Foundation of China {[}41730855, 41522301, 41473081]; National Key Research and Development Program of China {[}2016YFA0600802, 2017YFC0210101]; 11th Recruitment Program of Global Experts (the Thousand Talents Plan) for Young Professionals - central budget of China; US National Science Foundation Division of Environmental Biology {[}1026843, 1504006, 1637459]; Directorate For Geosciences {[}1504006] Funding Source: National Science Foundation; Division Of Environmental Biology; Direct For Biological Sciences {[}1637459, 1026843] Funding Source: National Science Foundation; Office of Polar Programs (OPP) {[}1504006] Funding Source: National Science Foundation; Grants-in-Aid for Scientific Research {[}17H06297, 26252020, 26550004, 20K20316] Funding Source: KAKEN},
