@article{ WOS:001461340200001,
  ADDRESS = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
  AUTHOR = {Boschi, S. and Cucini, C. and Cardaioli, E. and Frati, F. and Nardi, F. and Carapelli, A.},
  JOURNAL = {JOURNAL OF ZOOLOGY},
  MONTH = {JUN},
  NUMBER = {2},
  PAGES = {109-120},
  PUBLISHER = {WILEY},
  TITLE = {Population genetics and the role of dispersal barriers in the Antarctic springtail Kaylathalia klovstadi (Collembola, Isotomidae)},
  TYPE = {Article},
  VOLUME = {326},
  YEAR = {2025},
  ABSTRACT = {Isolation and extreme environmental conditions have significantly impacted terrestrial invertebrate biodiversity in Antarctica, with glacial cycles further limiting animal diversity to a small number of species. Among these, springtails are relatively abundant, though their taxonomic diversity remains low. Antarctic glaciers may serve as major dispersal barriers, leading to high levels of genetic divergence among isolated populations. In Victoria Land (continental Antarctica), the Tucker Glacier has been proposed as an example of such a barrier; however, different species or taxa may respond differently to the same geographical features. In this study, we tested whether geographic distance, major glacier tongues, and seawater channels influence the distribution of Kaylathalia klovstadi (Collembola; Isotomidae) in Northern Victoria Land. We conducted mitochondrial DNA haplotype analysis on fragments of cytochrome c oxidase subunit 1 (cox1) and ATP synthase 6 (atp6) in 62 individuals from six K. klovstadi populations. Our analysis provides new insights into the genetic diversity and distribution patterns of K. klovstadi by incorporating additional sampling sites, specimens, and molecular markers. We challenge previous views regarding the role of the Tucker Glacier as a significant physical barrier to springtail dispersal in Northern Victoria Land. Our findings suggest that the impact of physical barriers on the genetic diversity of Antarctic Collembola, including the species studied here, requires a more careful evaluation. L'isolamento e le condizioni ambientali estreme hanno avuto un impatto significativo sulla biodiversit \& agrave; degli invertebrati terrestri in Antartide, con i cicli glaciali che hanno ulteriormente limitato la diversit \& agrave; animale a un numero ridotto di specie. Tra queste, i collemboli sono relativamente abbondanti, sebbene la loro diversit \& agrave; tassonomica rimanga bassa. I ghiacciai antartici possono fungere da barriere significative per la dispersione delle specie, portando a livelli elevati di divergenza genetica tra popolazioni isolate. Nella Terra di Vittoria (Antartide continentale), il Ghiacciaio Tucker \& egrave; stato proposto come esempio di tali barriere; tuttavia, specie diverse di collemboli potrebbero essere influenzate in modo diverso dalle stesse caratteristiche geografiche. In questo studio, abbiamo valutato se la distanza geografica, i principali ghiacciai e i canali d'acqua marina influenzano la distribuzione di Kaylathalia klovstadi (Collembola; Isotomidae) nella parte settentrionale della Terra di Vittoria. Abbiamo condotto un'analisi aplotipica del DNA mitocondriale su frammenti della subunit \& agrave; 1 della citocromo c ossidasi (cox1) e della ATP sintasi 6 (atp6) in 62 esemplari provenienti da sei popolazioni di K. klovstadi. La nostra analisi fornisce nuove informazioni sulla diversit \& agrave; genetica e sui modelli di distribuzione di K. klovstadi, incorporando siti di campionamento aggiuntivi, nuovi esemplari e marcatori molecolari. I risultati mettono in discussione il ruolo del Ghiacciaio Tucker come barriera fisica significativa per la dispersione di alcune specie di collemboli nella parte settentrionale della Terra di Vittoria. I nostri risultati suggeriscono che l'impatto delle barriere fisiche sulla diversit \& agrave; genetica dei collemboli antartici, inclusa la specie studiata in questo caso, debba essere valutato pi \& ugrave; accuratamente.},
  LANGUAGE = {English},
  AFFILIATION = {Carapelli, A (Corresponding Author), Univ Siena, Dept Life Sci, Via Aldo Moro 2, I-53100 Siena, Italy. Boschi, S.; Cucini, C.; Cardaioli, E.; Frati, F.; Nardi, F.; Carapelli, A., Univ Siena, Dept Life Sci, Via Aldo Moro 2, I-53100 Siena, Italy. Frati, F.; Nardi, F.; Carapelli, A., Natl Biodivers Future Ctr NBFC, Palermo, Italy.},
  DOI = {10.1111/jzo.70012},
  ISSN = {0952-8369},
  EISSN = {1469-7998},
  KEYWORDS = {Victoria land; biogeography; Collembola; population genetics; mitochondrial DNA; dispersal barriers; Antarctica; glaciers},
  KEYWORDS.PLUS = {NORTHERN VICTORIA LAND; GRESSITTACANTHA-TERRANOVA; INVERTEBRATES; MOUNTAINS; DIVERSITY; HEXAPODA; ISLANDS; CHOICE},
  RESEARCH.AREAS = {Zoology},
  WEB.OF.SCIENCE.CATEGORIES = {Zoology},
  AUTHOR.EMAIL = {antonio.carapelli@unisi.it},
  AFFILIATIONS = {University of Siena; National Biodiversity Future Center},
  FUNDING.ACKNOWLEDGEMENT = {Italian Program of Research in Antarctica {[}PNRA16\_00234]; University of Siena},
  FUNDING.TEXT = {This study was funded by the Italian Program of Research in Antarctica (PNRA16\_00234) to A.C. Partial support was also provided by the University of Siena. We wish to thank Dr. Lilian Valentine Wekesa for her help with the lab work. We thank all the colleagues who produced some of the DNA data and published scientific articles which represented the starting point of the study. We are particularly grateful to the authors listed in the references: Stevens, Fjellberg, et al. (2006), Stevens and D'Haese (2017), and Collins et al. (2019). Without their efforts, the collected data and conclusions of our work would have been much less complete. We thank Dr. Barbara Valle for the realization of the photo of the graphical abstract. We also thank two anonymous Reviewers for constructive critical remarks that have substantially improved the quality of the final version of this manuscript. Open access publishing facilitated by Universita degli Studi di Siena, as part of the Wiley - CRUI-CARE agreement.},
