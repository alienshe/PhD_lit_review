  AFFILIATION = {Bubnova, EN (Corresponding Author), Moscow MV Lomonosov State Univ, Moscow 119234, Russia. Bubnova, E. N.; Konovalova, O. P., Moscow MV Lomonosov State Univ, Moscow 119234, Russia.},
  DOI = {10.1134/S1063074019020020},
  ISSN = {1063-0740},
  EISSN = {1608-3377},
  KEYWORDS = {marine fungi; Ascomycota; Basidiomycota; diversity; molecular methods; ITS; Arctic seas; Chukchi Sea; bottom sediments},
  KEYWORDS.PLUS = {GENETIC DIVERSITY; COMMUNITIES; PHYLOGENIES; NOV},
