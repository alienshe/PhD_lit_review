@article{ WOS:001431779600003,
Author = {Dragone, Nicholas B. and Childress, Mary K. and Vanderburgh, Caihong and
   Willmore, Rachel and Hogg, Ian D. and Sancho, Leopoldo G. and Lee,
   Charles K. and Barrett, John E. and Quandt, C. Alisha and Lemonte,
   Joshua J. and Adams, Byron J. and Fierer, Noah},
Title = {A comprehensive survey of soil microbial diversity across the Antarctic
   continent},
Journal = {POLAR BIOLOGY},
Year = {2025},
Volume = {48},
Number = {2},
Month = {JUN},
Abstract = {Antarctic soils are unique from those found nearly anywhere else on
   Earth yet can still harbor a broad diversity of microorganisms able to
   tolerate the challenging conditions typical of the continent. For these
   reasons, microbiologists have been drawn to Antarctica for decades.
   However, our understanding of which microbes thrive in Antarctic soils
   and how they to do so remains limited. To help resolve these knowledge
   gaps, we analyzed a collection of 200 archived Antarctic soils-from
   Livingston Island on the Antarctic Peninsula to Cape Hallett in northern
   Victoria Land. We analyzed the prokaryotic and fungal communities in
   these soils using both cultivation-independent marker gene sequencing
   and cultivation-dependent approaches (microbial isolation), paired with
   extensive soil geochemical analyses. Our cultivation-independent
   analyses indicate that colder, saltier, and drier soils harbor less
   diverse communities of bacteria and fungi, distinct from those found in
   soils with less challenging conditions. We also built a culture
   collection from a subset of these soils that encompasses more than 50
   bacterial and fungal genera, including cold-tolerant organisms, such as
   Cryobacterium and Cryomyces. By directly comparing the diversity of our
   cultured isolates against our cultivation-independent data, we show that
   many of the more abundant Antarctic taxa are not readily cultivated and
   highlight bacterial and fungal taxa that should be the focus of future
   cultivation efforts. Together, we hope that our collection of isolates,
   the comprehensive data compiled from the cultivation-independent
   analyses, and our geochemical analyses will serve as a community
   resource to accelerate the study of Antarctic soil microbes.},
