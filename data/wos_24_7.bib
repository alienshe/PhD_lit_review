@article{ WOS:000263468500006,
Author = {Arenz, Brett E. and Blanchette, Robert A.},
Title = {Investigations of fungal diversity in wooden structures and soils at
   historic sites on the Antarctic Peninsula},
Journal = {CANADIAN JOURNAL OF MICROBIOLOGY},
Year = {2009},
Volume = {55},
Number = {1},
Pages = {46-56},
Month = {JAN},
