@article{ WOS:A1978GM51900010,
Author = {BEHAN, VM and HILL, SB},
Title = {FEEDING-HABITS AND SPORE DISPERSAL OF ORIBATID MITES IN THE
   NORTH-AMERICAN ARCTIC},
Journal = {REVUE D ECOLOGIE ET DE BIOLOGIE DU SOL},
Year = {1978},
Volume = {15},
Number = {4},
Pages = {497-516},
Abstract = {Feeding habits of 25 spp. of oribatids from the North America arctic and
   subarctic were determined by gut content analysis. Over 50\% are
   panphytophages, a feeding habit considered to be ecologically
   advantageous, in that a particular species can survive in many different
   habitats. Microphytophagous species (11) were recorded, predominantly
   from the Damaeoidea, Eremaeoidea and Oppioidea. Individuals of 11 spp.
   of Oribatei were plated to examine the importance of mites in
   disseminating fungi. Fungal species (20) were identified from colonies
   growing on approximately 60\% of the plates. Most of the isolated fungi
   have a world-wide distribution, occurring in soils from tropical to
   arctic regions. Fecal pellets from 7 spp. were also plated. Fungal
   colonies grew from only 14\% of these pellets which may be a result of
   the feeding habits of the species examined. Most species being
   panphytophagous, the amount of fungal material in their gut would vary
   from meal to meal. The direct and indirect effects of oribatid mites on
   decomposition within the soil ecosystem is discussed.},
