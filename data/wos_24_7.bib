Type = {Article},
Language = {English},
Affiliation = {Harvard Univ, 22 Divin Ave, Cambridge, MA 02138 USA.
   Univ Cambridge, Dept Plant Sci, Cambridge CB2 3EA, England.
   British Antarctic Survey, NERC, Cambridge CB3 0ET, England.},
DOI = {10.2307/3761554},
ISSN = {0027-5514},
EISSN = {1557-2536},
