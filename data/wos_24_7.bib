@article{ WOS:A19621729B00004,
Author = {BOYD, WL and BOYD, JW},
Title = {FOOD SPOILAGE AND PRESERVATION IN ARCTIC},
Journal = {FOOD TECHNOLOGY},
Year = {1962},
Volume = {16},
Number = {2},
Pages = {91+},
Abstract = {Although atmospheric counts of bacteria and fungi taken on Petri plates
   containing various bacteriological media were found to be lower than in
   temperate regions, data indicate that microorganisms capable of causing
   food spoilage are present in arctic air. Certain isolates were
   characterized to genus and species; it is suggested that they are of
   water and/or soil origin. Refrigeration is necessary in arctic regions,
   especially during the summer, as exemplified by a food poisoning
   outbreak occurring when proper precautions were not observed.},
