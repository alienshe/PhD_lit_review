}


@Article{rayyan-273421408,
  AUTHOR = {Albores, S. and Sanguinedo, P. and Held, B.H. and Cerdeiras, M.P. and Blanchette, R.A.},
  JOURNAL = {Sydowia},
  NOTE = {Export Date: 26 June 2025; Cited By: 10 | RAYYAN-INCLUSION: {"Sylvie" = >"Included"}},
  PAGES = {185-192},
  TITLE = {Biodiversity and antimicrobial activity of antarctic fungi from the fildes peninsula, king george island},
