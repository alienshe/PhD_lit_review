  JOURNAL = {Extremophiles},
  NOTE = {Export Date: 26 June 2025; Cited By: 6 | RAYYAN-INCLUSION: {"Sylvie" = >"Included"}},
  NUMBER = {2},
  TITLE = {Diversity and enzymatic, biosurfactant and phytotoxic activities of culturable Ascomycota fungi present in marine sediments obtained near the South Shetland Islands, maritime Antarctica},
  VOLUME = {28},
  YEAR = {2024},
  URL = {https://www.scopus.com/inward/record.uri?eid = 2-s2.0-85188071212&doi = 10.1007%2fs00792-024-01336-4&partnerID = 40&md5 = 7fda77e5a9a7668515e3af82640b608a},
  KEYWORDS = {Emulsification, Extremophiles, Metabolites, Polar, Screening, Taxonomy, Antarctic Regions, Ascomycota, Cladosporium, Geologic Sediments, Pseudeurotium bakeri, Antarctica, Ascomycetes, Cladosporium, sediment},
  ABSTRACT = {We studied the culturable fungal community recovered from deep marine sediments in the maritime Antarctic, and assessed their capabilities to produce exoenzymes, emulsifiers and metabolites with phytotoxic activity. Sixty-eight Ascomycota fungal isolates were recovered and identified. The most abundant taxon recovered was the yeast Meyerozyma guilliermondii, followed by the filamentous fungi Penicillium chrysogenum, P. cf. palitans, Pseudeurotium cf. bakeri, Thelebolus balaustiformis, Antarctomyces psychrotrophicus and Cladosporium sp. Diversity indices displayed low values overall, with the highest values obtained at shallow depth, decreasing to the deepest location sampled. Only M. guilliermondii and P. cf. palitans were detected in the sediments at all depths sampled, and were the most abundant taxa at all sample sites. The most abundant enzymes detected were proteases, followed by invertases, cellulases, lipases, carrageenases, agarases, pectinases and esterases. Four isolates showed good biosurfactant activity, particularly the endemic species A. psychrotrophicus. Twenty-four isolates of P. cf. palitans displayed strong phytotoxic activities against the models Lactuca sativa and Allium schoenoprasum. The cultivable fungi recovered demonstrated good biosynthetic activity in the production of hydrolytic exoenzymes, biosurfactant molecules and metabolites with phytotoxic activity, reinforcing the importance of documenting the taxonomic, ecological and biotechnological properties of fungi present in deep oceanic sediments of the Southern Ocean. © The Author(s), under exclusive licence to Springer Nature Japan KK, part of Springer Nature 2024.},
  DOI = {10.1007/s00792-024-01336-4},
  INCLUSION = {Included},
  EXPORT_DATE = {26 June 2025},
  CITED_BY = {6}
}


@Article{rayyan-273421247,
  AUTHOR = {Teixeira, E.A.A. and de Souza, L.M.D. and Vieira, R. and Lirio, J.M. and Coria, S.H. and Convey, P. and Rosa, C.A. and Rosa, L.H.},
  JOURNAL = {Extremophiles},
  NOTE = {Export Date: 26 June 2025; Cited By: 2 | RAYYAN-INCLUSION: {"Sylvie" = >"Included"}},
  NUMBER = {2},
  TITLE = {Enzymes and biosurfactants of industrial interest produced by culturable fungi present in sediments of Boeckella Lake, Hope Bay, north-east Antarctic Peninsula},
  VOLUME = {28},
  YEAR = {2024},
  URL = {https://www.scopus.com/inward/record.uri?eid = 2-s2.0-85196619262&doi = 10.1007%2fs00792-024-01345-3&partnerID = 40&md5 = 98ae64dbac870cac3e42c644c32e1270},
  KEYWORDS = {Antarctica, Emulsification, Extremophiles, Polar, Primary metabolites, Taxonomy, Threatened environment, Antarctic Regions, Cellulase, Esterases, Fungal Proteins, Fungi, Geologic Sediments, Lakes, Surface-Active Agents, cellulase, esterase, fungal protein, surfactant, Antarctica, enzymology, fungus, isolation and purification, lake, metabolism, microbiology, sediment},
  ABSTRACT = {This study characterized cultivable fungi present in sediments obtained from Boeckella Lake, Hope Bay, in the north-east of the Antarctic Peninsula, and evaluated their production of enzymes and biosurfactants of potential industrial interest. A total of 116 fungal isolates were obtained, which were classified into 16 genera within the phyla Ascomycota, Basidiomycota and Mortierellomycota, in rank. The most abundant genera of filamentous fungi included Pseudogymnoascus, Pseudeurotium and Antarctomyces; for yeasts, Thelebolales and Naganishia taxa were dominant. Overall, the lake sediments exhibited high fungal diversity and moderate richness and dominance. The enzymes esterase, cellulase and protease were the most abundantly produced by these fungi. Ramgea cf. ozimecii, Holtermanniella wattica, Leucosporidium creatinivorum, Leucosporidium sp., Mrakia blollopis, Naganishia sp. and Phenoliferia sp. displayed enzymatic index > 2. Fourteen isolates of filamentous fungi demonstrated an Emulsification Index 24% (EI24%) ≥ 50%; among them, three isolates of A. psychrotrophicus showed an EI24% > 80%. Boeckella Lake itself is in the process of drying out due to the impact of regional climate change, and may be lost completely in approaching decades, therefore hosts a threatened community of cultivable fungi that produce important biomolecules with potential application in biotechnological processes. © The Author(s), under exclusive licence to Springer Nature Japan KK 2024.},
  DOI = {10.1007/s00792-024-01345-3},
  INCLUSION = {Included},
  EXPORT_DATE = {26 June 2025},
  CITED_BY = {2}
}


