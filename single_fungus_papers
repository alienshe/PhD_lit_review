@Article{rayyan-273421437,
  AUTHOR = {Vieira, G. and Purić, J. and Morão, L.G. and dos Santos, J.A. and Inforsato, F.J. and Sette, L.D. and Ferreira, H. and Sass, D.C.},
  JOURNAL = {Letters in Applied Microbiology},
  NOTE = {Export Date: 26 June 2025; Cited By: 22 | RAYYAN-INCLUSION: {"Sylvie" = >"Maybe"} | RAYYAN-LABELS: single fungus},
  NUMBER = {1},
  PAGES = {64-71},
  TITLE = {Terrestrial and marine Antarctic fungi extracts active against Xanthomonas citri subsp. citri},
  VOLUME = {67},
  YEAR = {2018},
  URL = {https://www.scopus.com/inward/record.uri?eid = 2-s2.0-85048495276&doi = 10.1111%2flam.12890&partnerID = 40&md5 = ffad5c15ae940e1a3fa30f191eb14fad},
  KEYWORDS = {Antarctic fungi, antibacterial action, citrus canker, secondary metabolites, Xanthomonas, Antarctic Regions, Anti-Bacterial Agents, Antibiosis, Cell Extracts, Citrus, Fungi, Geologic Sediments, Microbial Sensitivity Tests, Plant Diseases, Soil Microbiology, Xanthomonas, Antarctica, Bacteria (microorganisms), Cadophora, Citrus, Fungi, Paraconiothyrium, Penicillium, Pseudogymnoascus, Xanthomonas, Xanthomonas citri, Biomolecules, Cell death, Metabolites, Microorganisms, RNA, Sediments, Submarine geology, fungal DNA, fungal extract, genomic DNA, antiinfective agent, cell extract, Antarctic fungus, Antarctica, Antibacterial action, Citrus canker, Filamentous fungus, Marine sediments, Plant pathogen, Secondary metabolites, Soil/sediment, Xanthomonas, antimicrobial activity, bacterial disease, bacterium, cell, genetic resource, growth, inhibition, marine environment, pathogen, secondary metabolite, taxonomy, terrestrial environment, Antarctica, antibacterial activity, Article, bacterial growth, Cadophora, cell death, cell viability, controlled study, fungal strain, fungus, fungus identification, genetic resource, in vitro study, marine species, MIC90, minimum bactericidal concentration, minimum inhibitory concentration, nonhuman, Paraconiothyrium, Penicillium, plant disease, Pseudogymnoascus, terrestrial species, Toxicocladosporium, Xanthomonas citri, antibiosis, Citrus, genetics, growth, development and aging, metabolism, microbial sensitivity test, microbiology, physiology, sediment, Xanthomonas, Fungi},
  ABSTRACT = {This study aims to obtain secondary metabolites extracts from filamentous fungi isolated from soil and marine sediments from Antarctica and assess its potential antibacterial activity on Xanthomonas citri subsp. citri, the agent of citrus canker. Metabolites production was conducted in Malt 2% broth at 15°C for 20 days after which intracellular and extracellular extracts were obtained. The extracts were evaluated by cell viability assays through Resazurin Microtitre Assay. From 158 fungal extracts, 33 hampered bacterial growth in vitro. The average inhibition of the extracts obtained from terrestrial (soil) and marine (sediments) fungi was 94 and 97% respectively. These inhibition values were close to the average of 90% cell death for the positive control. MIC90 and MBC for the bioactive extracts were established. Isolates that produced active metabolites against the phytopathogen were identified using molecular taxonomy (ITS-rRNA sequencing) as: Pseudogymnoascus, Penicillium, Cadophora, Paraconiothyrium and Toxicocladosporium. Antarctic fungal strains isolated from terrestrial and marine sediments were able to produce secondary metabolites with antimicrobial activity against X. citri subsp. citri, highlighting the importance of these microbial genetic resources. These metabolites have potential to be used as alternatives for the control of this plant pathogen. Significance and Impact of the Study: This manuscript makes an impact on the study of micro-organisms from extreme habitats and their possible contribution in discovering new active molecules against pathogens of agricultural interest. Studies on the Antarctic continent and its communities have attracted the scientific community due to the long period of isolation and low levels of disturbance that surrounds the region. Knowing the potential of fungi in this region to produce active secondary metabolites, we aim to contribute to the discovery of compounds with antibacterial action in Xanthomonas citri subsp. citri, a plant pathogen present in several regions around the globe. © 2018 The Society for Applied Microbiology},
  DOI = {10.1111/lam.12890},
  INCLUSION = {Maybe},
  EXPORT_DATE = {26 June 2025},
  CITED_BY = {22},
  TAGS = {single fungus}
