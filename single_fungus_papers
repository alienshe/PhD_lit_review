@Article{rayyan-273421576,
  AUTHOR = {Selbmann, L. and Isola, D. and Zucconi, L. and Onofri, S.},
  JOURNAL = {Fungal Biology},
  NOTE = {Export Date: 26 June 2025; Cited By: 72 | RAYYAN-INCLUSION: {"Sylvie" = >"Maybe"} | RAYYAN-LABELS: single fungus},
  NUMBER = {10},
  PAGES = {937-944},
  TITLE = {Resistance to UV-B induced DNA damage in extreme-tolerant cryptoendolithic Antarctic fungi: Detection by PCR assays},
  VOLUME = {115},
  YEAR = {2011},
  URL = {https://www.scopus.com/inward/record.uri?eid = 2-s2.0-80053181954&doi = 10.1016%2fj.funbio.2011.02.016&partnerID = 40&md5 = 9b3c5e0e9d5c98cf5dbae5130b5e5711},
  KEYWORDS = {Black meristematic fungi, DNA damage, PCR, RAPD, UV-B resistance, Antarctic Regions, Ascomycota, Base Sequence, DNA Damage, Geologic Sediments, Molecular Sequence Data, Polymerase Chain Reaction, Random Amplified Polymorphic DNA Technique, Ultraviolet Rays, Antarctica, East Antarctica, McMurdo Dry Valleys, Cryomyces, Cryomyces antarcticus, Cryomyces minteri, Fungi, Saccharomyces pastorianus, bioassay, dose-response relationship, endolithon, fungus, genome, ice free corridor, inhibition, irradiation, pollution exposure, pollution tolerance, polymerase chain reaction, polymorphism, ultraviolet B radiation, yeast, Antarctica, article, Ascomycetes, DNA damage, genetics, isolation and purification, microbiology, molecular genetics, nucleotide sequence, polymerase chain reaction, radiation exposure, random amplified polymorphic DNA, sediment, ultraviolet radiation},
  ABSTRACT = {Cryptoendolithic Antarctic black fungi are adapted to the harshest terrestrial conditions as in the ice-free area of the McMurdo Dry Valleys. Recently, surviving space simulated conditions proves their bewildering extremotolerance. In order to investigate the potential DNA damage and their response after UV-B exposition, two strains of Antarctic cryptoendolithic black fungi, Cryomyces antarcticus CCFEE 534 and Cryomyces minteri CCFEE 5187, were irradiated at different UV-B doses. Since conventional methods cannot be applied to these organisms, the effect on the genome was assessed by RAPD and rDNA amplification PCR based assays; the results were compared with the responses of Saccharomyces pastorianus DBVPG 6283 treated with the same conditions. Results showed that template activity was drastically inhibited in S. pastorianus after irradiation. Dramatic changes in the RAPD profiles showed after 30. min of exposure while the rDNA amplification of SSU, LSU, and ITS portions failed after 30, 60, and 90. min of exposure respectively. No alteration was detected in the templates of the Antarctic strains where both RAPD profiles and rDNA PCR amplifications were unaffected even after 240. min of exposure. The electroferograms of the rDNA portions of Cryomyces strains were perfectly readable and conserved whilst the analyses revealed a marked alteration in S. pastorianus confirming the high resistance of the Antarctic strains to UV-B exposure. © 2011 British Mycological Society.},
  DOI = {10.1016/j.funbio.2011.02.016},
  INCLUSION = {Maybe},
  EXPORT_DATE = {26 June 2025},
  CITED_BY = {72},
  TAGS = {single fungus}
