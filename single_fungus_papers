@Article{rayyan-273421199,
  AUTHOR = {Gerginova, M. and Stoyanova, K. and Peneva, N. and Dincheva, I. and Alexieva, Z.},
  JOURNAL = {Processes},
  NOTE = {Export Date: 26 June 2025; Cited By: 1 | RAYYAN-INCLUSION: {"Sylvie" = >"Maybe"} | RAYYAN-LABELS: single fungus},
  NUMBER = {8},
  TITLE = {An Investigation into the Potential of a Penicillium Commune Strain to Eliminate Aromatic Compounds},
  VOLUME = {11},
  YEAR = {2023},
  URL = {https://www.scopus.com/inward/record.uri?eid = 2-s2.0-85169076253&doi = 10.3390%2fpr11082402&partnerID = 40&md5 = eba0cbf1a23a04869755ebb175952025},
  KEYWORDS = {biodegradation, DNA sequencing, fungi, GC–MS, oxiganases, PAHs, Anthracene, Aromatization, DNA, DNA sequences, Gene encoding, Metabolites, Naphthalene, Phenols, Sewage, Water pollution, Antarctic soils, Clean waters, DNA Sequencing, GC–MS, Industrial wastewaters, Monophenols, Oxiganase, Polluted water, Sewage water, Soil water, Biodegradation},
  ABSTRACT = {The quantity of industrially polluted waters is increasing everywhere, of which a significant part is occupied by a number of mono- and poly-aromatic compounds. Toxins enter the soil, sewage, and clean water by mixing with or seeping into them from industrial wastewater. By using 18S RNA and ITS sequences, the Penicillium commune AL5 strain that was isolated from Antarctic soil was identified. This study is dedicated to exploring its capacity to metabolize hazardous aromatic compounds. The strain showed very good potential in the degradation of hydroxylated monophenols and possessed exceptional abilities in terms of resorcinol degradation. The strain’s ability to metabolize 0.3 g/L of p-cresol at 10 °C is notable. The strain is also capable of metabolizing LMW PAHs (naphthalene, anthracene, and phenanthrene) and eliminating all three tested compounds under 23 °C, respectively, 77.5%, 93.8%, and 75.1%. At 10 °C, the process slowed down, but the degradation of naphthalene continued to be over 50%. The quantity of PAH and a few significant intermediary metabolites were determined using GC–MS analysis. Sequencing of the enzymes phenol hydroxylase and catechol 1,2-dioxygenase revealed a close association with the genes and proteins in some fungal strains that can degrade the aromatic compounds examined thus far. © 2023 by the authors.},
  DOI = {10.3390/pr11082402},
  INCLUSION = {Maybe},
  EXPORT_DATE = {26 June 2025},
  CITED_BY = {1},
  TAGS = {single fungus}
