  URL = {https://www.scopus.com/inward/record.uri?eid = 2-s2.0-84864625885&doi = 10.3402%2fpolar.v30i0.7283&partnerID = 40&md5 = a8d836d4a6a6bb57711e7bc5addfdb39},
