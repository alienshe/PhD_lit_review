@Article{rayyan-273421311,
  AUTHOR = {Krumova, E. and Abrashev, R. and Dishliyska, V. and Stoyancheva, G. and Kostadinova, N. and Miteva-Staleva, J. and Spasova, B. and Angelova, M.},
  JOURNAL = {Journal of Basic Microbiology},
  NOTE = {Export Date: 26 June 2025; Cited By: 7 | RAYYAN-INCLUSION: {"Sylvie" = >"Maybe"} | RAYYAN-LABELS: single fungus},
  NUMBER = {9},
  PAGES = {782-794},
  TITLE = {Cold-active catalase from the psychrotolerant fungus Penicillium griseofulvum},
  VOLUME = {61},
  YEAR = {2021},
  URL = {https://www.scopus.com/inward/record.uri?eid = 2-s2.0-85111417644&doi = 10.1002%2fjobm.202100209&partnerID = 40&md5 = 703a682489106315f0847d0ab5d8bcb0},
  KEYWORDS = {Antarctic fungi, cold-active catalase, cold-adapted properties, improved enzyme production, laboratory technology, Antarctic Regions, Biotechnology, Catalase, Cold Temperature, Hydrogen-Ion Concentration, Oxidative Stress, Penicillium, Temperature, catalase, Antarctica, biotechnology, cold, enzymology, genetics, growth, development and aging, isolation and purification, metabolism, oxidative stress, Penicillium, pH, procedures, temperature},
  ABSTRACT = {Cold-active catalase (CAT) elicits great interest because of its vast prospective at the medical, commercial, and biotechnological levels. The study paper reports the production of cold-active CAT by the strain Penicillium griseofulvum P29 isolated from Antarctic soil. Improved enzyme production was achieved by optimization of medium and culture conditions. Maximum CAT was demonstrated under low glucose content (2%), 10% inoculum size, temperature 20°C, and dissolved oxygen concentration (DO) 40%. An effective laboratory technology based on changing the oxidative stress level through an increase of DO in the bioreactor was developed. The used strategy resulted in a 1.7- and 1.4-fold enhanced total enzyme activity and maximum enzyme productivity. The enzyme was purified and characterized. P. griseofulvum P29 CAT was most active at approximately 20°C and pH 6.0. Its thermostability was in the range between 5°C and 40°C. © 2021 Wiley-VCH GmbH},
  DOI = {10.1002/jobm.202100209},
  INCLUSION = {Maybe},
  EXPORT_DATE = {26 June 2025},
  CITED_BY = {7},
  TAGS = {single fungus}
