  AUTHOR = {Connell, L. and Staudigel, H.},
  JOURNAL = {Biology},
  NOTE = {Export Date: 26 June 2025; Cited By: 45 | RAYYAN-INCLUSION: {"Sylvie" = >"Maybe"} | RAYYAN-LABELS: single fungus},
