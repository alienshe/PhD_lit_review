  TITLE = {Survival, metabolic activity, and ultrastructural damages of Antarctic black fungus in perchlorates media},
  VOLUME = {13},
  YEAR = {2022},
  URL = {https://www.scopus.com/inward/record.uri?eid = 2-s2.0-85143904646&doi = 10.3389%2ffmicb.2022.992077&partnerID = 40&md5 = 95c2b1f732a19b4b88fd0984955897d1},
