  VOLUME = {18},
  YEAR = {1997},
  URL = {https://www.scopus.com/inward/record.uri?eid = 2-s2.0-0030797210&doi = 10.1007%2fs003000050167&partnerID = 40&md5 = 13d97107a485af8676688a299f151eb0},
